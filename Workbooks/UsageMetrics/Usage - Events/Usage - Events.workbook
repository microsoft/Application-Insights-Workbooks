            "query": "let table = datatable(Value:string)[{parseCohortEvents}]\r\n| where Value startswith\"{\\\"cohort\\\":\"\r\n| project name = \"cohorts\", Cohorts = strcat(\"(\",parse_json(Value).cohort, \"  | summarize makeset(name)\");\r\ntable\r\n| summarize cohorts = make_list(Cohorts) by tostring(name)\r\n| project cohorts= replace(@'\\[\"', @'union \\[ \\1', tostring(cohorts))\r\n| project cohorts= replace(@'name\\)\"', @'name)) \\1', cohorts)\r\n| project cohorts= replace(@',\"\\(', @',( \\1', cohorts)\r\n| project cohorts= replace(@'\\]', @'\\1', cohorts)\r\n| project cohorts= replace(@'\\\\\"', @'\"\\1', cohorts)",

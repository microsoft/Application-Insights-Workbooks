            "criteriaData": [
              {
                "condition": "if (Step1FilterDimension1 is empty ), result = '2'",
                "criteriaContext": {
                  "leftOperand": "Step1FilterDimension1",
                  "operator": "is Empty",
                  "rightValType": "static",
                  "rightVal": "",

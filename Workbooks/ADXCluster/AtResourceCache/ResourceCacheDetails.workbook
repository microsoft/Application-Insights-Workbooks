                        "useGrouping": false
                      },
                      "emptyValCustomText": "-"
                    }
                  },
                  {
                    "columnMatch": "difference2",
                    "formatter": 18,
                    "formatOptions": {
                      "thresholdsOptions": "icons",
                      "thresholdsGrid": [
                        {
                          "operator": "==",
                          "thresholdValue": "e",
                          "representation": "Check",
                          "text": "The actual usage lookback is equal to the cache policy"
                        },
                        {
                          "operator": "==",
                          "thresholdValue": "i",

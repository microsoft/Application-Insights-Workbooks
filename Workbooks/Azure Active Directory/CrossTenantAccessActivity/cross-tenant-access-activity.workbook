                                "formatOptions": {
                                  "customColumnWidthSetting": "60%"
                                }
                              }
                            ],
                            "labelSettings": [
                              {
                                "columnId": "statusCount",
                                "label": "Status Count"
                              },
                              {
                                "columnId": "status",
                                "label": "Status"

          "locInfoColumn": "Location",
          "sizeSettings": "Count",
          "sizeAggregation": "Sum",
          "labelSettings": "Location",
          "legendMetric": "Count",
          "legendAggregation": "Sum",
          "itemColorSettings": {
            "nodeColorField": "Count",
            "colorAggregation": "Sum",
            "type": "heatmap",
            "heatmapPalette": "greenRed"
          }
        }
      },
      "customWidth": "60",
      "name": "LocationRiskyIP"
    },

    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let data = SigninLogs\r\n| extend AppDisplayName = iff(AppDisplayName == '', 'Unknown', AppDisplayName)\r\n| where AppDisplayName in ('*') or '*' in ('*')\r\n| extend CAStatus = case(ConditionalAccessStatus ==0,\"Success\",ConditionalAccessStatus == 1, \"Failure\", ConditionalAccessStatus == 2, \"Not Applied\", ConditionalAccessStatus == \"\", \"Not Applied\", \"Unknown\");\r\nlet caData = data\r\n| summarize Count = count() by CAStatus\r\n| join kind = inner (data\r\n| make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by CAStatus\r\n| project-away TimeGenerated) on CAStatus\r\n| order by Count desc, CAStatus asc\r\n| project CAStatus, Count, Trend\r\n| serialize Id = row_number();\r\ndata\r\n| summarize Count = count() by CAStatus,AppDisplayName\r\n| join kind=inner (data\r\n| make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by CAStatus,AppDisplayName\r\n| project-away TimeGenerated) on CAStatus, AppDisplayName\r\n| order by Count desc, AppDisplayName asc\r\n| project CAStatus, AppDisplayName, Count, Trend\r\n| serialize Id = row_number(1000000)\r\n| join kind=inner (caData) on CAStatus\r\n| project Id, ConditionalAccessStatus = AppDisplayName, Type = '‚öôÔ∏è App' , ['Sign-in Count'] = Count, Trend, ParentId = Id1\r\n| union (caData \r\n| project Id, ConditionalAccessStatus = CAStatus, Type = 'üìç CA Status', ['Sign-in Count'] = Count, Trend, ParentId = -1)\r\n| order by ['Sign-in Count'] desc, ConditionalAccessStatus asc",

    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let data = SigninLogs\r\n| where AppDisplayName in ({Apps}) or '*' in ({Apps});\r\nlet appData = data\r\n| summarize Count = count() by Os = tostring(DeviceDetail.operatingSystem)\r\n| where Os != ''\r\n| join kind=inner (data\r\n    | make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by Os = tostring(DeviceDetail.operatingSystem)\r\n    | project-away TimeGenerated) on Os\r\n| order by Count desc, Os asc\r\n| project Os, Count, Trend\r\n| serialize Id = row_number();\r\ndata\r\n| summarize Count = count() by Os = tostring(DeviceDetail.operatingSystem), Browser = tostring(DeviceDetail.browser)\r\n| join kind=inner (data\r\n    | make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by Os = tostring(DeviceDetail.operatingSystem), Browser = tostring(DeviceDetail.browser)\r\n    | project-away TimeGenerated) on Os, Browser\r\n| order by Count desc, Os asc\r\n| project Os, Browser, Count, Trend\r\n| serialize Id = row_number(1000000)\r\n| join kind=inner (appData) on Os\r\n| project Id, Name = Browser, Type = 'üåê Browser', ['Sign-in Count'] = Count, Trend, ParentId = Id1\r\n| union (appData \r\n    | project Id, Name = Os, Type = 'üñ•Ô∏è OS', ['Sign-in Count'] = Count, Trend, ParentId = -1)\r\n| order by ['Sign-in Count'] desc, Name asc",

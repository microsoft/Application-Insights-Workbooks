                                "text": "{0}{1}"
                              },
                              {
                                "operator": ">=",
                                "thresholdValue": "85",
                                "representation": "4",
                                "text": "{0}{1}"
                              },
                              {
                                "operator": "Default",
                                "thresholdValue": null,
                                "representation": "success",
                                "text": "{0}{1}"

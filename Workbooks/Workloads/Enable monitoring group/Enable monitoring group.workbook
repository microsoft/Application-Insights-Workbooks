              "formatter": 18,
              "formatOptions": {
                "thresholdsOptions": "icons",
                "thresholdsGrid": [
                  {
                    "operator": "==",
                    "thresholdValue": "Monitored",
                    "representation": "success",
                    "text": "Enabled"
                  },
                  {

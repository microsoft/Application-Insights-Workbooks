        "query": "customMetrics\n| where name in (\"preview.item.dropped.count\", \"preview.item.success.count\")\n| where isempty('{SdkVersion}') or '{SdkVersion}' == 'All' or sdkVersion == '{SdkVersion}'\n| extend drop_reason = iff(name == \"preview.item.dropped.count\", tostring(customDimensions[\"drop.reason\"]), \"\")\n| extend drop_code = iff(name == \"preview.item.dropped.count\", tostring(customDimensions[\"drop.code\"]), \"\")\n| where \n    // If drop reason filter is empty or 'All', show both success and filtered dropped\n    (isempty('{DropReason}') or '{DropReason}' == 'All') and (\n        name == \"preview.item.success.count\" or \n        (name == \"preview.item.dropped.count\" and (isempty('{DropReason}') or '{DropReason}' == 'All' or drop_reason == '{DropReason}') and (isempty('{DropCode}') or '{DropCode}' == 'All' or drop_code == '{DropCode}'))\n    ) or \n    // If specific drop reason is selected, show only dropped count with that reason\n    (isnotempty('{DropReason}') and '{DropReason}' != 'All') and (\n        name == \"preview.item.dropped.count\" and drop_reason == '{DropReason}' and (isempty('{DropCode}') or '{DropCode}' == 'All' or drop_code == '{DropCode}')\n    )\n| summarize total_count = sum(todouble(value)) by name, bin(timestamp, {TimeRange:grain})\n| extend display_name = case(\n    name == \"preview.item.success.count\", \"Success Count\",\n    name == \"preview.item.dropped.count\", \"Dropped Count\",\n    name\n)\n| project timestamp, metric_type = display_name, count_value = total_count\n| render timechart",
        "size": 1,
        "title": "Telemetry Export Success vs. Failure (Line Chart)",
        "timeContext": {
          "durationMs": 0,
          "createdTime": "2025-07-30T00:00:00.000Z"

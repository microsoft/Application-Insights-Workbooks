      "content": {
        "version": "KqlItem/1.0",
        "query": "let SnapshotTime = datetime({_SnapshotTime});\nUCClient\n| where TimeGenerated == SnapshotTime\n| summarize count() by Result = OSEdition",
        "size": 1,
        "title": "OS Edition",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",

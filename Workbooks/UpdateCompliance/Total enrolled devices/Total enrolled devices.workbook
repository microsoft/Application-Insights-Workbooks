    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let SnapshotTime = datetime({_SnapshotTime});\nUCClient\n| where TimeGenerated == SnapshotTime\n| summarize count() by OSVersion",
        "size": 1,
        "title": "OS Version",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [

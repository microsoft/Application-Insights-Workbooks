          {
            "id": "4aec8423-90f0-454c-8f65-1051434745dc",
            "version": "KqlParameterItem/1.0",
            "name": "_Description",
            "type": 1,
            "isRequired": true,
            "query": "let _SnapshotTime = datetime({_SnapshotTime});\r\nlet _ErrorCode = \"{_ErrorCode}\";\r\nlet _AlertSubtype = \"{_AlertSubtype}\";\r\nUCUpdateAlert\r\n| where TimeGenerated == _SnapshotTime\r\n| where ErrorCode == _ErrorCode  and AlertSubtype == _AlertSubtype\r\n| summarize arg_max(Description, *) by Description\r\n| project Description",
            "crossComponentResources": [
              "{mappedWorkspace}"
            ],
            "isHiddenWhenLocked": true,
            "queryType": 0,
            "resourceType": "microsoft.operationalinsights/workspaces"
          },
          {
            "id": "b25fd6fc-a7ce-4e64-b18e-9d3c28236ac2",
            "version": "KqlParameterItem/1.0",
            "name": "_Recommendation",
            "type": 1,
            "isRequired": true,
            "query": "let _SnapshotTime = datetime({_SnapshotTime});\r\nlet _ErrorCode = \"{_ErrorCode}\";\r\nlet _AlertSubtype = \"{_AlertSubtype}\";\r\nUCUpdateAlert\r\n| where TimeGenerated == _SnapshotTime\r\n| where ErrorCode == _ErrorCode and AlertSubtype == _AlertSubtype\r\n| summarize arg_max(Recommendation, *) by Recommendation\r\n| project Recommendation",
            "crossComponentResources": [
              "{mappedWorkspace}"
            ],
            "isHiddenWhenLocked": true,
            "queryType": 0,
            "resourceType": "microsoft.operationalinsights/workspaces"
          }

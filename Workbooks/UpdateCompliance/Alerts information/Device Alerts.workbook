    {
      "type": 12,
      "content": {
        "version": "NotebookGroup/1.0",
        "groupType": "editable",
        "title": "Update Alerts",
        "expandable": true,
        "expanded": true,
        "items": [
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "let _AzureADDeviceId = \"{_AzureADDeviceId}\";\r\nlet _UpdateCategory= \"{_UpdateCategory}\";\r\nlet _SnapshotTime = datetime({_SnapshotTime});\r\nUCUpdateAlert\r\n| where TimeGenerated == _SnapshotTime\r\n| where AlertStatus == \"Active\"\r\n| where AzureADDeviceId == _AzureADDeviceId and UpdateCategory == _UpdateCategory\r\n| extend Desc =  strcat(\"Description: \", tostring(Description))\r\n| extend Recom = strcat(\"Recommendation: \", tostring(Recommendation))\r\n| project AlertSubtype, AlertClassification, Desc, Recom, URL",
              "size": 3,
              "noDataMessage": "There are no Update Alerts",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",

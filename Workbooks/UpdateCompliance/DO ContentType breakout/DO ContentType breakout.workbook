			}
		},
		{
			"type": 9,
			"content": {
				"version": "KqlParameterItem/1.0",
				"crossComponentResources": [
					"{mappedWorkspace}"
				],
				"parameters": [
					{
						"id": "7a2bb320-fa9c-45c8-bbf6-942c8b3a4b0e",
						"version": "KqlParameterItem/1.0",
						"name": "mappedSubscription",
						"label": "subscription",
						"type": 6,
						"isRequired": true,
						"value": null,
						"isHiddenWhenLocked": true,
						"typeSettings": {
							"additionalResourceOptions": [],
							"includeAll": true,
							"showDefault": false
						}
					},
					{
						"id": "01c6d66e-3f87-4e8a-8be6-0accbe4f4036",
						"version": "KqlParameterItem/1.0",
						"name": "mappedWorkspace",
						"label": "workspace",
						"type": 5,
						"isRequired": true,
						"query": "resources\r\n| where type == \"microsoft.operationalinsights/workspaces\"\r\n| where \"{mappedSubscription:subscriptionId}\" == subscriptionId\r\n| project id",
						"crossComponentResources": [
							"{mappedSubscription}"
						],
						"value": null,
						"isHiddenWhenLocked": true,
						"typeSettings": {
							"additionalResourceOptions": [],
							"showDefault": false
						},
						"queryType": 1,
						"resourceType": "microsoft.resourcegraph/resources"
					},
					{
						"id": "e0d2d74e-bd88-4208-bb51-10a5491a47bb",
						"version": "KqlParameterItem/1.0",
						"name": "DOSnapshotTime",
						"label": "Snapshot Time",
						"type": 1,
						"description": "Time indicating the latest snapshot",
						"isRequired": true,
						"query": "(UCDOStatus\r\n| where TimeGenerated >= ago(7d)\r\n| distinct TimeGenerated)\r\n|join kind = inner  (UCDOAggregatedStatus\r\n| where TimeGenerated >= ago(7d)\r\n| distinct TimeGenerated) on TimeGenerated\r\n| summarize max(TimeGenerated)",
						"crossComponentResources": [
							"{mappedWorkspace}"
						],
						"isHiddenWhenLocked": true,
						"queryType": 0,
						"resourceType": "microsoft.operationalinsights/workspaces",
						"value": null
					},
					{
						"id": "f56d6943-b8f5-449e-aaaf-06100a59e8b5",
						"version": "KqlParameterItem/1.0",
						"name": "_WorkbookContext",
						"label": "Workbook Context",
						"type": 1,
						"description": "Whether workbook is opened standalone or as part of the Hub.",
						"isRequired": true,
						"query": "print(\"Focused\")",
						"isHiddenWhenLocked": true,
						"timeContext": {
							"durationMs": 86400000
						},
						"queryType": 0,
						"resourceType": "microsoft.operationalinsights/workspaces"
					},
					{
						"id": "6fdfde4e-da31-4aa8-a3fd-acbd7b81e755",
						"version": "KqlParameterItem/1.0",
						"name": "ContentType",
						"type": 1,
						"isRequired": true,
						"crossComponentResources": [
							"{mappedWorkspace}"
						]
					},
					{
						"id": "5cd36655-0307-49d5-9b09-3e7a37514a9f",
						"version": "KqlParameterItem/1.0",
						"name": "AzureADTenantId",
						"label": "AzureADTenantId",
						"type": 2,
						"description": "Displays the target versions selected",
						"isRequired": true,
						"multiSelect": true,
						"quote": "'",
						"delimiter": ",",
						"query": "let _SnapshotTime = datetime({DOSnapshotTime});\r\nlet ContentTypeParam = \"{ContentType}\";\r\n\r\nUCDOStatus \r\n| where ContentType == ContentTypeParam \r\n| where TimeGenerated == _SnapshotTime \r\n| distinct AzureADTenantId",
						"crossComponentResources": [
							"{mappedWorkspace}"
						],
						"value": [
							"value::all"
						],
						"typeSettings": {
							"additionalResourceOptions": [
								"value::all"
							],
							"selectAllValue": "ALL",
							"showDefault": false
						},
						"defaultValue": "value::all",
						"queryType": 0,
						"resourceType": "microsoft.operationalinsights/workspaces"
					},
					{
						"id": "5cd36655-0307-49d5-9b09-3e7a37514a9g",
						"version": "KqlParameterItem/1.0",
						"name": "GroupID",
						"label": "GroupID",
						"type": 2,
						"description": "Displays the target versions selected",
						"isRequired": true,
						"multiSelect": true,
						"quote": "'",
						"delimiter": ",",
						"query": "let _SnapshotTime = datetime({DOSnapshotTime});\r\nlet ContentTypeParam = \"{ContentType}\";\r\n\r\nUCDOStatus\r\n| where ContentType == ContentTypeParam\r\n| where TimeGenerated == _SnapshotTime \r\n| extend GroupID = iff(isempty(GroupID) , 'None' , GroupID)\r\n| distinct GroupID",
						"crossComponentResources": [
							"{mappedWorkspace}"
						],
						"value": [
							"value::all"
						],
						"typeSettings": {
							"additionalResourceOptions": [
								"value::all"
							],
							"selectAllValue": "ALL",
							"showDefault": false
						},
						"defaultValue": "value::all",
						"queryType": 0,
						"resourceType": "microsoft.operationalinsights/workspaces"
					}
				],
				"style": "pills",
				"queryType": 0,
				"resourceType": "microsoft.operationalinsights/workspaces"
			},
			"name": "workbookParameters"
		},
		{
			"type": 3,
			"content": {
				"version": "KqlItem/1.0",
				"query": "let ContentTypeParam = \"{ContentType}\";\r\nlet _SnapshotTime = datetime({DOSnapshotTime});\r\nlet GroupIDSelected = \"{GroupID}\";\r\nlet AzureADTenantIdSelected = \"{AzureADTenantId}\";\r\n\r\nUCDOStatus\r\n| where TimeGenerated == _SnapshotTime\r\n| where ContentType == ContentTypeParam \r\n| extend FilterGroupID = iff(isempty(GroupID) , 'None' , GroupID)\r\n| where iff(GroupIDSelected has \"ALL\", true, GroupIDSelected has tostring(FilterGroupID))\r\n| where iff(AzureADTenantIdSelected has \"ALL\", true, AzureADTenantIdSelected has tostring(AzureADTenantId))\r\n| project AzureADTenantId , ContentType , DeviceName , GlobalDeviceId , GroupID , BytesFromCDN , BytesFromPeers,BytesFromGroupPeers ",
				"size": 0,
				"queryType": 0,
				"showAnalytics": true,
				"showExportToExcel": true,
				"exportToExcelOptions": "all",
				"resourceType": "microsoft.operationalinsights/workspaces",
				"crossComponentResources": [
					"{mappedWorkspace}"
				]
			},
			"name": "ContentTypeQuery"
		}
	],
	"$schema": "https://github.com/Microsoft/Application-Insights-Workbooks/blob/master/schema/workbook.json"
}
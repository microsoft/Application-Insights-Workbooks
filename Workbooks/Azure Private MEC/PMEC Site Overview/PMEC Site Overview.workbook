            "version": "KqlParameterItem/1.0",
            "name": "pccp",
            "label": "Packet Core",
            "type": 5,
            "isRequired": true,
            "query": "resources\r\n| where type =~ \"microsoft.mobilenetwork/packetcorecontrolplanes\"\r\n| where properties.sites[0].id =~ \"{site}\"\r\n| project id, selected=\"true\"",

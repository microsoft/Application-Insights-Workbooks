    {
      "type": 9,
      "content": {
        "version": "KqlParameterItem/1.0",
        "parameters": [
          {
            "id": "2c51791d-7825-4637-af1a-73e7a0d346cc",
            "version": "KqlParameterItem/1.0",
            "name": "replicasSampleSubtype",
            "label": "Sample type",
            "type": 10,
            "isRequired": true,
            "typeSettings": {
              "additionalResourceOptions": [],
              "showDefault": false
            },
            "jsonData": "[{\"value\":\"all\",\"label\":\"All\"},{\"value\":\"interesting\",\"label\":\"Notable\"}]",

              "showDefault": false
            },
            "jsonData": "[\r\n    {\"value\":\"all\",\"label\":\"All\"},\r\n    {\"value\":\"user\",\"label\":\"User\"},\r\n    {\"value\":\"system\",\"label\":\"System\"},\r\n    {\"value\":\"tempdb\",\"label\":\"tempdb\"}\r\n]",

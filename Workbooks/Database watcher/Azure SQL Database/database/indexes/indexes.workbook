                "showDefault": false
              },
              "jsonData": "[\r\n    {\"value\":1,\"label\":\"1\"},\r\n    {\"value\":10,\"label\":\"10\"},\r\n    {\"value\":50,\"label\":\"50\"},\r\n    {\"value\":100,\"label\":\"100\"},\r\n    {\"value\":200,\"label\":\"200\"}\r\n]",
              "timeContext": {

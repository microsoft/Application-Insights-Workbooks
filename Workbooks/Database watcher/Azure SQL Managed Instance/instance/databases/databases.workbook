                "criteriaContext": {
                  "operator": "Default",
                  "resultValType": "param",
                  "resultVal": "databaseLatestSampleTime"
                }
              }
            ],
            "timeContext": {
              "durationMs": 86400000
            }
          }
        ],
        "style": "formHorizontal",
        "queryType": 9
      },

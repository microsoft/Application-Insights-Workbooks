          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "{\"version\":\"AzureDataExplorerQuery/1.0\",\"queryText\":\"let user_databases = (\\r\\nsqlmi_database_properties\\r\\n| where sample_time_utc between (\\r\\n                                iif(({timeRange:end} - {timeRange:start}) <= 1h, ({timeRange:start} - 1h), {timeRange:start})\\r\\n                                ..\\r\\n                                iif(({timeRange:end} - {timeRange:start}) <= 1h, ({timeRange:end} + 1h), {timeRange:end})\\r\\n                                ) // Expand the range if selected range is shorter than the least frequent collection interval (1h)\\r\\n| where managed_instance_name =~ @\\\"{managedInstanceName}\\\"\\r\\n| where ({haReplica} and replica_type == \\\"HA secondary\\\") or (not ({haReplica}) and replica_type != \\\"HA secondary\\\")\\r\\n| where database_name !in~ ('master','tempdb','model','msdb') // do not consider \\\"distribution\\\" and \\\"ssisdb\\\" system databases in this context\\r\\n| summarize arg_max(collection_time_utc, *) by database_name\\r\\n| summarize count_user_databases = count(),\\r\\n            key = int(1)\\r\\n);\\r\\nsqlmi_instance_properties\\r\\n| where sample_time_utc between (\\r\\n                                iif(({timeRange:end} - {timeRange:start}) <= 1h, ({timeRange:start} - 1h), {timeRange:start})\\r\\n                                ..\\r\\n                                iif(({timeRange:end} - {timeRange:start}) <= 1h, ({timeRange:end} + 1h), {timeRange:end})\\r\\n                                ) // Expand the range if selected range is shorter than the least frequent collection interval (1h)\\r\\n| where managed_instance_name =~ @\\\"{managedInstanceName}\\\"\\r\\n| where ({haReplica} and replica_type == \\\"HA secondary\\\") or (not ({haReplica}) and replica_type != \\\"HA secondary\\\")\\r\\n| top 1 by sample_time_utc desc\\r\\n| extend key = int(1)\\r\\n| join kind=leftouter user_databases on key\\r\\n| extend replica_type_indicator = case(\\r\\nreplica_type == \\\"Primary\\\", \\\"🔵\\\",\\r\\nreplica_type == \\\"HA secondary\\\", \\\"🔘\\\",\\r\\nreplica_type == \\\"Geo-replication forwarder\\\", \\\"🟣\\\",\\r\\nreplica_type == \\\"Named secondary\\\", \\\"🟢\\\",\\r\\n\\\"\\\"\\r\\n),\\r\\nproduct_major_version = toint(split(product_version, \\\".\\\").[0])\\r\\n| project managed_instance_name = tostring(split(managed_instance_name, \\\".\\\")[0]),\\r\\nuptime = format_timespan(1s * datetime_diff(\\\"second\\\", sample_time_utc, database_engine_start_time_utc), \\\"d.hh:mm:ss\\\"),\\r\\nservice_tier = case(service_tier =~ \\\"GeneralPurpose\\\", \\\"General Purpose\\\", service_tier =~ \\\"BusinessCritical\\\", \\\"Business Critical\\\", service_tier),\\r\\nlogical_cpu_count,\\r\\ndatabase_engine_memory_mb,\\r\\nreserved_storage_mb,\\r\\nreplica_type = strcat(replica_type_indicator, replica_type),\\r\\nhardware_generation,\\r\\nproduct_update_policy = case(\\r\\n                            product_update_type == \\\"CU\\\" and product_major_version == 16, \\\"SQL Server 2022\\\",\\r\\n                            product_update_type == \\\"CU\\\" and product_major_version > 16, \\\"SQL Server vNext\\\",\\r\\n                            product_update_type == \\\"Continuous\\\", \\\"Always up-to-date\\\",\\r\\n                            \\\"\\\"),\\r\\nproduct_update_level,\\r\\ncount_user_databases,\\r\\nsample_age = datetime_diff(\\\"second\\\", now(), sample_time_utc),\\r\\nrt_tooltip = case(\\r\\nreplica_type == \\\"Primary\\\", \\\"The primary replica always exists for any managed instance. Primary replicas support read and write workloads. All other replica types support read workloads only.\\\",\\r\\nreplica_type == \\\"Geo-replication forwarder\\\", \\\"A geo-replication secondary replica, also known as a geo-replication forwarder, is created when you configure a failover group. Data from the primary replica is replicated to the geo-replication forwarder.\\\",\\r\\nreplica_type == \\\"HA secondary\\\", \\\"For primary and geo-replication forwarder, data may be replicated to an HA secondary replica to provide high availability (HA). HA secondary replicas do not have their own names, but inherit the name of the replica for which they provide high availability.\\\",\\r\\n\\\"\\\"\\r\\n),\\r\\nproduct_version,\\r\\ninstance_name_long = managed_instance_name\",\"clusterName\":\"{adxClusterUri}\",\"databaseName\":\"{adxDatabase}\"}",
              "size": 3,
              "noDataMessage": "There is no data for specified parameters.",
              "queryType": 9,
              "visualization": "table",
              "gridSettings": {
                "formatters": [
                  {
                    "columnMatch": "managed_instance_name",

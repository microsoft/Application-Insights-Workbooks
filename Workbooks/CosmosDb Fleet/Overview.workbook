    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "alertsmanagementresources  \r\n| where type =~ 'microsoft.alertsmanagement/alerts'   \r\n| where todatetime(properties.essentials.startDateTime) {TimeRange} //and todatetime(properties.essentials.startDateTime) < now()  \r\n| project Severity = tostring(properties.essentials.severity),  \r\nMonitorCondition = tostring(properties.essentials.monitorCondition),  \r\nObjectState = tostring(properties.essentials.alertState),  \r\nMonitorService = tostring(properties.essentials.monitorService),  \r\nAlertRuleId = tostring(properties.essentials.alertRule),  \r\nSignalType = tostring(properties.essentials.signalType),  \r\nTargetResource = tostring(properties.essentials.targetResourceName), \r\nTargetResourceType = tostring(properties.essentials.targetResourceName), id \r\n| summarize AlertsCount = count() by Severity, MonitorService , TargetResourceType\r\n| where TargetResourceType in~ (split(\"{Resources:label}\", \", \"))",
        "size": 1,
        "title": "Azure Monitor Alerts",
        "queryType": 1,
        "resourceType": "microsoft.resourcegraph/resources",
        "crossComponentResources": [
          "value::all"
        ]
      },
      "customWidth": "50",
      "name": "Alerts Management"

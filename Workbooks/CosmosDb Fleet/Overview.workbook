        "version": "KqlItem/1.0",
        "query": "advisorresources\r\n| where type == \"microsoft.advisor/recommendations\"\r\n| where properties contains \"documentdb\"\r\n| where id in~ ({Resources:value})\r\n| extend d = parse_json(properties)\r\n| extend Category = d.category, Impact = d.impact, ResourceId = tostring(id), Recommendation=d.shortDescription.solution\r\n| project Account=tostring(ResourceId), tostring(Category), tostring(Impact), tostring(Recommendation), id\r\n| extend DatabaseAccount=split(Account, \"/\")[8]\r\n| extend Link = split(Account, \"/providers/Microsoft.Advisor/\")[0]\r\n| project Link, DatabaseAccount, Category, Recommendation\r\n",
        "size": 1,
        "title": "Azure Advisor Recommendations",

    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "// Count of Unique Users\nlet UniqueUsers = \nNetworkAccessTraffic\n| where UserPrincipalName in ({Users}) or '*' in ({Users})\n| where TrafficType in (\"Internet\", \"Private\", \"microsoft365\")\n| where TimeGenerated > ago(30d)\n| summarize UniqueUsers=dcount(UserPrincipalName)\n| extend snapshot = \"Unique Users\"\n| project col1 = UniqueUsers, snapshot;\n\n// Count of Unique Devices\nlet UniqueDevices = \nNetworkAccessTraffic\n| where UserPrincipalName in ({Users}) or '*' in ({Users})\n| where TrafficType in (\"Internet\", \"Private\", \"microsoft365\")\n| where TimeGenerated > ago(30d)\n| summarize UniqueDevices=dcount(DeviceId)\n| extend snapshot = \"Unique Devices\"\n| project col1 = UniqueDevices, snapshot;\n\n// Total Devices with Internet Access\nlet TotalInternetAccess = \nNetworkAccessTraffic\n| where UserPrincipalName in ({Users}) or '*' in ({Users})\n| where TrafficType == \"Internet\"\n| where TimeGenerated > ago(30d)\n| summarize TotalInternetAccess=dcount(DeviceId)\n| extend snapshot = \"Total Internet Access (Devices)\"\n| project col1 = TotalInternetAccess, snapshot;\n\n// Total Devices with Private Access\nlet TotalPrivateAccess = \nNetworkAccessTraffic\n| where UserPrincipalName in ({Users}) or '*' in ({Users})\n| where TrafficType == \"Private\"\n| where TimeGenerated > ago(30d)\n| summarize TotalPrivateAccess=dcount(DeviceId)\n| extend snapshot = \"Total Private Access (Devices)\"\n| project col1 = TotalPrivateAccess, snapshot;\n\n// Total Devices with Microsoft 365 Access\nlet TotalM365Access = \nNetworkAccessTraffic\n| where UserPrincipalName in ({Users}) or '*' in ({Users})\n| where TrafficType == \"microsoft365\"\n| where TimeGenerated > ago(30d)\n| summarize TotalM365Access=dcount(DeviceId)\n| extend snapshot = \"Total Microsoft 365 Access (Devices)\"\n| project col1 = TotalM365Access, snapshot;\n\n// Combine Results\nunion UniqueUsers, UniqueDevices, TotalInternetAccess, TotalPrivateAccess, TotalM365Access\n| order by snapshot\n",
        "size": 1,
        "timeContextFromParameter": "TimeRange",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [
          "{LogAnalyticWorkspace}"
        ],
        "visualization": "tiles",
        "tileSettings": {

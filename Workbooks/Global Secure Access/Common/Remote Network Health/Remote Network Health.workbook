    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "RemoteNetworkHealthLogs\r\n| project \r\n    [\"Time Generated\"] = TimeGenerated, \r\n    [\"Remote Network ID\"] = RemoteNetworkId, \r\n    [\"Source IP\"] = SourceIp, \r\n    [\"Destination IP\"] = DestinationIp, \r\n    [\"Received Bytes\"] = ReceivedBytes, \r\n    [\"Sent Bytes\"] = SentBytes, \r\n    Status = Status\r\n",
        "size": 3,
        "title": "Log",
        "timeContextFromParameter": "TimeRange",
        "showRefreshButton": true,
        "showExportToExcel": true,
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [
          "{LogAnalyticWorkspace}"
        ],
        "gridSettings": {
          "rowLimit": 1000,
          "filter": true
        }
      },
      "conditionalVisibility": {
        "parameterName": "tabSel",
        "comparison": "isEqualTo",
        "value": "AllTraffic"
      },
      "name": "query - 6"
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "// Calculate Healthy Device Links\nlet HealthyDeviceLinks = RemoteNetworkHealthLogs\n| where Status == \"BranchAlive\"\n| summarize by RemoteNetworkId, SourceIp, DestinationIp;\n\n// Calculate All Device Links\nlet AllDeviceLinks = RemoteNetworkHealthLogs\n| summarize by RemoteNetworkId, SourceIp, DestinationIp;\n\n// Calculate Unhealthy Device Links with updated logic for status\nlet UnhealthyDeviceLinks = RemoteNetworkHealthLogs\n| summarize arg_max(TimeGenerated, *) by RemoteNetworkId, SourceIp, DestinationIp\n| extend Status = iif(Status == \"BranchAlive\" and TimeGenerated >= ago(30m), \"üü¢ Healthy\", \"‚ùå Unhealthy\")\n| where Status == \"‚ùå Unhealthy\";\n\n// Total Remote Network\nlet TotalRemoteNetwork = RemoteNetworkHealthLogs\n| summarize ActiveBranches = dcount(RemoteNetworkId)\n| extend snapshot = \"Total Remote Network\"\n| project col1 = ActiveBranches, snapshot;\n\n// Total Device Links\nlet TotalDeviceLinks = RemoteNetworkHealthLogs\n| summarize TotalDeviceLinks = dcount(strcat(tostring(RemoteNetworkId), tostring(SourceIp), tostring(DestinationIp)))\n| extend snapshot = \"Total Device Links\"\n| project col1 = tolong(TotalDeviceLinks), snapshot;\n\n// Unhealthy Remote Networks\nlet UnhealthyRemoteNetworks = UnhealthyDeviceLinks\n| summarize UnhealthyRemoteNetworks = count()\n| extend snapshot = \"Unhealthy Remote Networks\"\n| project col1 = UnhealthyRemoteNetworks, snapshot;\n\n// Union of all the metrics\nTotalRemoteNetwork\n| union TotalDeviceLinks\n| union UnhealthyRemoteNetworks\n| order by snapshot\n",
        "size": 1,
        "timeContextFromParameter": "TimeRange",
        "queryType": 0,
        "resourceType": "microsoft.operationalinsights/workspaces",
        "crossComponentResources": [
          "{LogAnalyticWorkspace}"
        ],
        "visualization": "tiles",
        "tileSettings": {
          "titleContent": {
            "columnMatch": "snapshot",
            "formatter": 1
          },
          "leftContent": {
            "columnMatch": "col1",
            "formatter": 12,
            "formatOptions": {
              "palette": "auto"
            }
          },
          "showBorder": true,
          "sortCriteriaField": "snapshot",
          "size": "auto"
        },
        "mapSettings": {
          "locInfo": "LatLong",
          "sizeSettings": "ExistingClients",
          "sizeAggregation": "Sum",
          "legendMetric": "ExistingClients",
          "legendAggregation": "Sum",
          "itemColorSettings": {
            "type": "heatmap",
            "colorAggregation": "Sum",
            "nodeColorField": "ExistingClients",
            "heatmapPalette": "greenRed"
          }
        },
        "textSettings": {
          "style": "bignumber"
        }
      },
      "conditionalVisibility": {
        "parameterName": "tabSel",

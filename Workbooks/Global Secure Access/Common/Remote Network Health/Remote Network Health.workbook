      "name": "query - 2"
    },
    {
      "type": 12,
      "content": {
        "version": "NotebookGroup/1.0",
        "groupType": "editable",
        "items": [
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "let GeoInfoTable = \r\n    RemoteNetworkHealthLogs\r\n    | extend GeoInfo = geo_info_from_ip_address(SourceIp) \r\n    | project TimeGenerated, SourceIp, Country = tostring(GeoInfo.country), State = tostring(GeoInfo.state), City = tostring(GeoInfo.city), Latitude = tostring(GeoInfo.latitude), Longitude = tostring(GeoInfo.longitude)\r\n    | where Country != \"\";\r\n\r\nlet UnhealthyNetworks = \r\n    RemoteNetworkHealthLogs\r\n    | summarize arg_max(TimeGenerated, *) by RemoteNetworkId, SourceIp, DestinationIp\r\n    | extend Status = case(Status == \"BranchAlive\", \"üü¢ Healthy\", \"‚ùå Unhealthy\") \r\n    | where Status == \"‚ùå Unhealthy\";\r\n\r\nGeoInfoTable\r\n| join kind=inner (UnhealthyNetworks) on SourceIp\r\n| summarize Count = count() by City, State, Country, Status\r\n",
              "size": 3,
              "title": "Locations",
              "timeContextFromParameter": "TimeRange",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
                "{LogAnalyticWorkspace}"
              ],
              "visualization": "map",
              "mapSettings": {
                "locInfo": "CountryRegion",
                "locInfoColumn": "Country",
                "latitude": "Latitude",
                "longitude": "Longitude",
                "sizeAggregation": "Sum",
                "labelSettings": "Country",
                "legendMetric": "Country",
                "legendAggregation": "Count",
                "itemColorSettings": {
                  "nodeColorField": "Count",
                  "colorAggregation": "Sum",
                  "type": "heatmap",
                  "heatmapPalette": "greenRed"
                }
              }
            },
            "name": "query - 0"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "let DeviceLinkHourlyBytes = RemoteNetworkHealthLogs\r\n| make-series [\"Sent Bytes Trend\"] = sum(SentBytes), [\"Received Bytes Trend\"] = sum(ReceivedBytes) on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by [\"Remote Network ID\"] = RemoteNetworkId, [\"Source IP\"] = SourceIp, [\"Destination IP\"] = DestinationIp;\r\n\r\nlet LatestDeviceLinkStatus = RemoteNetworkHealthLogs\r\n| summarize arg_max(TimeGenerated, *) by RemoteNetworkId, SourceIp, DestinationIp\r\n| extend Status = iif(Status == \"BranchAlive\" and TimeGenerated >= ago(30m), \"üü¢ Healthy\", \"‚ùå Unhealthy\")\r\n| project [\"Remote Network ID\"] = RemoteNetworkId, [\"Source IP\"] = SourceIp, [\"Destination IP\"] = DestinationIp, Status, [\"Last Operation\"] = Status, [\"Last Event Time\"] = TimeGenerated;\r\n\r\nLatestDeviceLinkStatus\r\n| join kind=inner (DeviceLinkHourlyBytes) on [\"Remote Network ID\"], [\"Source IP\"], [\"Destination IP\"]\r\n| project [\"Remote Network ID\"], [\"Source IP\"], [\"Destination IP\"], Status, [\"Last Operation\"], [\"Last Event Time\"], [\"Sent Bytes Trend\"], [\"Received Bytes Trend\"]\r\n| order by [\"Remote Network ID\"]\r\n",
              "size": 3,
              "title": "List Of Remote Networks",
              "timeContextFromParameter": "TimeRange",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
                "{LogAnalyticWorkspace}"
              ],
              "gridSettings": {
                "formatters": [
                  {
                    "columnMatch": "Sent",
                    "formatter": 9,
                    "formatOptions": {
                      "palette": "blue"
                    }
                  },
                  {
                    "columnMatch": "Received Bytes Trend",
                    "formatter": 9,
                    "formatOptions": {
                      "palette": "blue"
                    }
                  },
                  {
                    "columnMatch": "LastOperation",
                    "formatter": 5
                  },
                  {
                    "columnMatch": "LastEventTime",
                    "formatter": 5
                  },
                  {
                    "columnMatch": "ReceivedBytesTrend",
                    "formatter": 9,
                    "formatOptions": {
                      "palette": "blue"
                    }
                  },
                  {
                    "columnMatch": "Count",
                    "formatter": 4,
                    "formatOptions": {
                      "palette": "magenta"
                    }
                  },
                  {
                    "columnMatch": "Recived",
                    "formatter": 4,
                    "formatOptions": {
                      "palette": "turquoise"
                    }
                  },
                  {
                    "columnMatch": "Total",
                    "formatter": 4,
                    "formatOptions": {
                      "palette": "pink"
                    }
                  }
                ]
              },
              "sortBy": []
            },
            "name": "query - 1"
          }
        ]
      },
      "conditionalVisibility": {
        "parameterName": "tabSel",
        "comparison": "isEqualTo",
        "value": "Overview"
      },
      "name": "group - 4"
    },
    {
      "type": 12,
      "content": {
        "version": "NotebookGroup/1.0",
        "groupType": "editable",
        "items": [
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "RemoteNetworkHealthLogs\r\n| summarize SentBytesSum = sum(SentBytes), ReceivedBytesSum = sum(ReceivedBytes) by bin(TimeGenerated, 1h), RemoteNetworkId, SourceIp, DestinationIp\r\n| extend TotalBytesSum = SentBytesSum + ReceivedBytesSum\r\n| project RemoteNetworkId = RemoteNetworkId, SourceIp, DestinationIP = DestinationIp, TimeGenerated, TotalBytesSum\r\n| order by RemoteNetworkId\r\n",
              "size": 2,
              "title": "Usage Over Time (GB)",
              "timeContextFromParameter": "TimeRange",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
                "{LogAnalyticWorkspace}"
              ],
              "visualization": "barchart"
            },
            "conditionalVisibility": {
              "parameterName": "tabSel",
              "comparison": "isEqualTo",
              "value": "Overview"
            },
            "name": "query - 0"
          }
        ]
      },
      "name": "group - 5"
    }

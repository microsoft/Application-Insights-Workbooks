    {
      "type": 1,
      "content": {
        "json": "### Legacy Authentications Sign-in Errors"
      },
      "conditionalVisibility": null
    },
    {
      "type": 3,
      "content": {
        "version": "KqlItem/1.0",
        "query": "let data = SigninLogs \r\n|extend ClientAppUsed = iff(isempty(ClientAppUsed)==true,\"Unknown\" ,ClientAppUsed)  \r\n|extend isLegacyAuth = case(ClientAppUsed contains \"Browser\", \"No\", ClientAppUsed contains \"Mobile Apps and Desktop clients\", \"No\", ClientAppUsed contains \"Exchange ActiveSync\", \"No\", ClientAppUsed contains \"Other clients\", \"Yes\", \"Unknown\") \r\n| extend ErrorCode = tostring(Status.errorCode) \r\n| extend Reason = tostring(Status.failureReason) \r\n| where ErrorCode !in (\"0\",\"50058\",\"50148\",\"50140\", \"51006\", \"50059\", \"65001\", \"52004\", \"50055\", \"50144\",\"50072\", \"50074\", \"16000\",\"16001\", \"16003\", \"50127\", \"50125\", \"50129\",\"50143\", \"81010\", \"81014\", \"81012\")\r\n|where isLegacyAuth==\"Yes\"\r\n|where AppDisplayName in ({Apps}) or '*' in ({Apps})\r\n|where UserDisplayName in ({Users}) or '*' in ({Users});\r\n\r\nlet legacyData = data\r\n| summarize Count = count() by AppDisplayName, UserDisplayName, ErrorCode, Reason\r\n| join kind = inner\r\n(\r\n    data\r\n    | make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by AppDisplayName\r\n    | project-away TimeGenerated\r\n)\r\non AppDisplayName \r\n| order by Count desc, AppDisplayName asc, UserDisplayName asc, ErrorCode asc, Reason asc\r\n| project AppDisplayName , UserDisplayName, ErrorCode, Reason, Count, Trend\r\n| serialize Id = row_number();\r\ndata\r\n| summarize Count = count() by AppDisplayName, UserDisplayName, ErrorCode, Reason\r\n| join kind=inner\r\n(\r\n    data\r\n    | make-series Trend = count() default = 0 on TimeGenerated in range({TimeRange:start}, {TimeRange:end}, {TimeRange:grain}) by AppDisplayName\r\n    | project-away TimeGenerated\r\n)\r\non  AppDisplayName\r\n| order by Count desc, AppDisplayName asc, UserDisplayName asc, ErrorCode asc, Reason asc\r\n| project AppDisplayName ,UserDisplayName,ErrorCode, Reason, Count, Trend\r\n| serialize Id = row_number(1000000)\r\n| join kind=inner\r\n(\r\n    legacyData\r\n)\r\non AppDisplayName \r\n| project Id, Name = AppDisplayName,  Type = '‚öôÔ∏è App' ,  ['Sign-in Count'] = Count, Trend, ParentId = Id1\r\n| order by ['Sign-in Count'] desc, Name\r\n| union (legacyData\r\n| project Id, Name  = UserDisplayName, Type = 'üë§ User', ['Error Code'] = ErrorCode, Reason, ['Sign-in Count'] = Count, Trend, ParentId = -1)\r\n| order by ['Sign-in Count'] desc, Name\r\n",

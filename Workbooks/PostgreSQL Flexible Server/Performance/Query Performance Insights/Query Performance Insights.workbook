DatabaseName), strcat(\"oid::\", DatabaseId), DatabaseName), ['user identifier'] = UserId, ['window start'] = WindowStart, ['window end'] = WindowEnd, ['mean IO time'] = MeanIOtime, ['minimum IO time'] = MinIOtime, ['maximum IO time'] = MaxIOtime, ['mean execution duration'] = MeanExecutionTime, ['minimum execution duration'] = MinExecutionTime, ['maximum execution duration'] = MaxExecutionTime, ['total execution duration'] = TotalExecutionTime, ['mean data read'] = MeanDataRead, ['mean data write'] = MeanDataWrite, ['mean rows'] = MeanRows, ['total calls'] = TotalCalls, ['total rows'] = TotalRows, ['total IO time'] = TotalIOtime\r\n",

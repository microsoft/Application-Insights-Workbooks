                "formatters": [
                  {
                    "columnMatch": "Weight",
                    "formatter": 18,
                    "formatOptions": {
                      "thresholdsOptions": "colors",
                      "thresholdsGrid": [
                        {
                          "operator": ">=",
                          "thresholdValue": "3.5",
                          "representation": "redBright",
                          "text": "[\"Weight\"]"
                        },
                        {
                          "operator": "<",
                          "thresholdValue": "3.5",
                          "representation": "lightBlue",
                          "text": "[\"Weight\"]"
                        },
                        {
                          "operator": "Default",
                          "thresholdValue": null,
                          "text": "{0}{1}"
                        }
                      ]
                    }
                  },
                  {
                    "columnMatch": "RecommendationId",
                    "formatter": 5
                  },
                  {
                    "columnMatch": "Description",
                    "formatter": 5
                  },
                  {
                    "columnMatch": "TimeGenerated",
                    "formatter": 5
                  }
                ],
                "filter": true,
                "sortBy": [
                  {
                    "itemKey": "$gen_thresholds_Weight_2",
                    "sortOrder": 1
                  }
                ]
              },
              "sortBy": [
                {
                  "itemKey": "$gen_thresholds_Weight_2",
                  "sortOrder": 1
                }
              ]
            },
            "customWidth": "50",
            "showPin": false,
            "name": "PriortizedRecommendations-grid",
            "styleSettings": {
              "showBorder": true
            }
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "SCOMAssessmentRecommendation\r\n| where FocusArea has \"{FocusArea}\"\r\nand RecommendationResult != \"Inconclusive\" and RecommendationResult == \"Failed\"\r\nand TimeGenerated > ago(30d)\r\n| extend High = (RecommendationScore >= 35)\r\n| extend Low = (RecommendationScore < 35)\r\n| extend status = iff(High==true,\"High Priority Recommendations\",\"Low Priority Recommendations\")\r\n| extend Passed = (RecommendationResult == \"Passed\")\r\n//| extend Priority = iif((RecommendationScore >= 35 and RecommendationResult == \"Failed\"),\"High Priority\",\"Low Priority\")\r\n//| extend Priority1 = iif((RecommendationScore < 35 and RecommendationResult == \"Failed\"),\"Low Priority\",\"Low Priority\")\r\n//| extend Passed = iif(RecommendationResult == \"Passed\",\"Passed\",\"Failed\")\r\n| summarize count() by status\r\n\r\n",
              "size": 0,
              "title": "{FocusArea}",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "visualization": "piechart",
              "sortBy": [],
              "chartSettings": {
                "seriesLabelSettings": [
                  {
                    "seriesName": "High Priority Recommendations",
                    "color": "redBright"
                  },
                  {
                    "seriesName": "Low Priority Recommendations",
                    "color": "blue"
                  }
                ]
              }
            },
            "customWidth": "50",
            "conditionalVisibility": {
              "parameterName": "FocusArea",
              "comparison": "isNotEqualTo"
            },
            "name": "FocusArea-chart",
            "styleSettings": {
              "margin": "0px"
            }
          }
        ],
        "exportParameters": true
      },
      "customWidth": "100",
      "name": "Recommendation-group"
    },

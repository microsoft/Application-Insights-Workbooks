        "items": [
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "SCOMAssessmentRecommendation\r\n|where FocusArea != \"EnvironementFilter\"  and RecommendationResult == \"Failed\"\r\nand TimeGenerated {TimeRange}\r\n| summarize by   Recommendation, FocusArea, Weight = (RecommendationScore/10), RecommendationId, Description\r\n//| order by FocusArea asc",
              "size": 0,
              "title": "Prioritized Recommendations",
              "noDataMessage": "No recommendations found",
              "exportedParameters": [
                {
                  "fieldName": "RecommendationId",
                  "parameterName": "RecommendationId",
                  "parameterType": 1
                },
                {
                  "fieldName": "Recommendation",
                  "parameterName": "Recommendation",
                  "parameterType": 1
                },
                {
                  "fieldName": "FocusArea",

            "type": 1,
            "isHiddenWhenLocked": true,
            "criteriaData": [
              {
                "condition": "if (virtualHubs is not empty ), result = 'where id in~ ({virtualHubs})'",
                "criteriaContext": {
                  "leftOperand": "virtualHubs",
                  "operator": "isNotNull",
                  "rightValType": "param",
                  "resultValType": "static",
                  "resultVal": "where id in~ ({virtualHubs})"

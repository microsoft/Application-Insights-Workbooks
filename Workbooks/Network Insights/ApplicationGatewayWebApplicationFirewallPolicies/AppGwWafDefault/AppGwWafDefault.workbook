                        },
                        {
                          "columnMatch": "access_log",
                          "formatter": 7,
                          "formatOptions": {
                            "linkTarget": "OpenBlade",
                            "linkLabel": "Find in Access Log",
                            "linkIsContextBlade": true,
                            "bladeOpenContext": {
                              "bladeName": "LogsBlade",
                              "extensionName": "Microsoft_Azure_Monitoring_Logs",
                              "bladeParameters": [
                                {
                                  "name": "source",
                                  "source": "static",
                                  "value": "Microsoft_Azure_Monitoring_Logs/LogsBlade"
                                },
                                {
                                  "name": "query",
                                  "source": "cell",
                                  "value": ""

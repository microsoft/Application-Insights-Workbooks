                        "parameterName": "HttpStatus",
                        "parameterType": 1
                      },
                      {
                        "fieldName": "transactionId_g",
                        "parameterName": "TransactionId",
                        "parameterType": 1
                      }
                    ],
                    "queryType": 0,
                    "resourceType": "microsoft.network/applicationgateways",
                    "crossComponentResources": [
                      "{Associations}"
                    ],
                    "gridSettings": {
                      "formatters": [
                        {
                          "columnMatch": "requestUri_s",
                          "formatter": 5
                        },
                        {
                          "columnMatch": "httpMethod_s1",
                          "formatter": 5
                        },
                        {

                            "linkIsContextBlade": true,
                            "bladeOpenContext": {
                              "bladeName": "LogsBlade",
                              "extensionName": "Microsoft_Azure_Monitoring_Logs",
                              "bladeParameters": [
                                {
                                  "name": "source",
                                  "source": "static",
                                  "value": "Microsoft_Azure_Monitoring_Logs/LogsBlade"
                                },
                                {
                                  "name": "query",
                                  "source": "cell",

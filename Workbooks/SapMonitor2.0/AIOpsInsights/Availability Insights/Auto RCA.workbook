            "jsonData": "[\r\n    {\"value\":\"Summary\",\r\n    \"label\":\"Process availability\"},\r\n    {\"value\":\"Process\",\r\n    \"label\":\"SAP platform\"},\r\n    {\"value\":\"VM\", \r\n    \"label\": \"Azure platform\"}\r\n]",

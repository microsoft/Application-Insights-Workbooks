                                    "style": "decimal",
                                    "maximumSignificantDigits": 1
                                  }
                                }
                              },
                              {
                                "columnMatch": "DBThresholdBreach",
                                "formatter": 18,
                                "formatOptions": {
                                  "thresholdsOptions": "colors",
                                  "thresholdsGrid": [
                                    {
                                      "operator": "==",
                                      "thresholdValue": "true",
                                      "representation": "redBright",

            "version": "KqlParameterItem/1.0",
            "name": "EndTime",
            "type": 1,
            "query": "print iff(isnotempty(\"{AlertFired}\"), datetime_add(\"minute\", 30, todatetime(\"{AlertFired}\")), {TimeRange:end})",
            "crossComponentResources": [

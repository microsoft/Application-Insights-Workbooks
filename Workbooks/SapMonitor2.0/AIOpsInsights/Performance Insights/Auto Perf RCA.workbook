                                    },
                                    {
                                      "operator": "Default",
                                      "thresholdValue": null,
                                      "text": "[\"DBTimeDisplay\"]"
                                    }
                                  ]
                                }
                              },
                              {
                                "columnMatch": "DBTime",
                                "formatter": 5,
                                "numberFormat": {

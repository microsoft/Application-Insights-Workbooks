            "content": {
              "version": "KqlItem/1.0",
              "query": "{\"version\":\"Merge/1.0\",\"merges\":[{\"id\":\"fc463697-ea02-4145-b41d-51dcddf6e716\",\"mergeType\":\"inner\",\"leftTable\":\"Alerts\",\"rightTable\":\"Providers\",\"leftColumn\":\"ProviderName\",\"rightColumn\":\"ProviderName\"}],\"projectRename\":[{\"originalName\":\"[Alerts].Name\",\"mergedName\":\"Alert\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].Severity\",\"mergedName\":\"Severity\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].AlertCondition\",\"mergedName\":\"AlertCondition\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].UserResponse\",\"mergedName\":\"UserResponse\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].Description\",\"mergedName\":\"Description\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Providers].ProviderCategory\",\"mergedName\":\"Category\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].FiredTime\",\"mergedName\":\"FiredTime\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].id\",\"mergedName\":\"id\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Alerts].ProviderName\",\"mergedName\":\"ProviderName\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Providers].ProviderID\",\"mergedName\":\"AffectedResource\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Providers].ProviderName\",\"mergedName\":\"ProviderName1\",\"fromId\":\"fc463697-ea02-4145-b41d-51dcddf6e716\"},{\"originalName\":\"[Providers].CategoryFilterText\",\"mergedName\":\"CategoryFilterText\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].a\",\"mergedName\":\"a\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].Column1\",\"mergedName\":\"Column1\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].b\",\"mergedName\":\"b\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].Column2\",\"mergedName\":\"Column2\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].SelectedProviderCategoryArray\",\"mergedName\":\"SelectedProviderCategoryArray\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].CategoryFilterArray\",\"mergedName\":\"CategoryFilterArray\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].FilterArray\",\"mergedName\":\"FilterArray\",\"fromId\":\"unknown\"},{\"originalName\":\"[Providers].t\",\"mergedName\":\"t\",\"fromId\":\"unknown\"}]}",

          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "print providersList = parse_json(\"{ProvidersList:escapejson}\")\r\n| mv-expand provider = providersList.value\r\n| where provider.properties.provisioningState == \"Succeeded\"\r\n| extend sidValue = iff(isnotnull(provider.properties.providerSettings.sapSid),\r\nprovider.properties.providerSettings.sapSid, provider.properties.providerSettings.sid)\r\n| extend ProviderType = provider.properties.providerSettings.providerType\r\n| where isnotnull(sidValue)\r\n| project id=provider.id, Name=provider.name, ProviderType=tostring(ProviderType), SID=tostring(sidValue)\r\n| where SID == '{SviResource:name}'\r\n| join kind = inner (\r\n    print config = parse_json('{ProviderCategoryMap}')\r\n    | mv-expand group = config\r\n    | project ProviderType=tostring(group.ProviderType), ProviderCategory=tostring(group.ProviderCategory)\r\n) on ProviderType\r\n| project-away ProviderType1\r\n| project ProviderID=id, ProviderName=Name, ProviderCategory, SelectedProviderCategoryArray = iff('{SelectedProviderCategory}' != '*', dynamic(['{SelectedProviderCategory}']), dynamic([])), CategoryFilterArray = pack_array({CategoryFilter})\r\n| extend FilterArray = iff(array_length(SelectedProviderCategoryArray) > 0, SelectedProviderCategoryArray, CategoryFilterArray)\r\n| where array_index_of(FilterArray, \"*\") != -1 or array_index_of(FilterArray, ProviderCategory) != -1\r\n| project-away FilterArray, SelectedProviderCategoryArray, CategoryFilterArray\r\n\r\n\r\n",
              "size": 0,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [

                  "name": "NW_Raw_Data",
                  "type": 1,
                  "query": "let nw_Availability = parse_json('{NW_Instance_Availability}');\r\nlet nw_replication = parse_json('{NW_Enqueue_Replication}');\r\nlet nw_errors = parse_json('{NW_SM21_Error_Count}');\r\nlet batch_job_errors = parse_json('{NW_Batch_Job_Error_Count}');\r\nprint x = 1\r\n| project group = array_concat(nw_Availability, nw_replication, nw_errors, batch_job_errors)",

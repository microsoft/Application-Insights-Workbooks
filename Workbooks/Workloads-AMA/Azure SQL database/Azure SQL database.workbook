                    "numberFormat": {
                      "unit": 17,
                      "options": {
                        "style": "decimal",
                        "useGrouping": false,
                        "minimumSignificantDigits": 1
                      }
                    }
                  },
                  {
                    "columnMatch": "Online trend",
                    "formatter": 21,
                    "formatOptions": {
                      "min": 0,
                      "palette": "green",
                      "showIcon": true
                    }
                  },
                  {

                    "formatter": 8,
                    "formatOptions": {
                      "min": 0,
                      "palette": "red",
                      "showIcon": true
                    },
                    "numberFormat": {
                      "unit": 17,
                      "options": {
                        "style": "decimal",
                        "maximumSignificantDigits": 1
                      }
                    }

                    "formatOptions": {
                      "min": 0,
                      "palette": "redBright",
                      "customColumnWidthSetting": "20ch"
                    }
                  },
                  {
                    "columnMatch": "AMAOnly",
                    "formatter": 4,
                    "formatOptions": {
                      "min": 0,
                      "palette": "green",
                      "customColumnWidthSetting": "20ch"
                    }
                  },
                  {
                    "columnMatch": "counts",
                    "formatter": 22,
                    "formatOptions": {
                      "compositeBarSettings": {
                        "labelText": "[\"migstatus\"]",
                        "columnSettings": [
                          {

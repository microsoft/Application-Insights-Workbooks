                        "type": 10,
                        "isRequired": true,
                        "value": "P1",
                        "typeSettings": {
                          "additionalResourceOptions": [],
                          "showDefault": false
                        },
                        "jsonData": "[\r\n { \"value\": \"P1\", \"label\": \"Defender for Servers Plan 1\"},\r\n { \"value\": \"P2\", \"label\": \"Defender for Servers Plan 2\"}\r\n]",
                        "timeContext": {

              "formatOptions": {
                "thresholdsOptions": "icons",
                "thresholdsGrid": [
                  {
                    "operator": ">",
                    "thresholdValue": "0",
                    "representation": "trendup",
                    "text": "{0}{1} increase over {TimeRange:label}"

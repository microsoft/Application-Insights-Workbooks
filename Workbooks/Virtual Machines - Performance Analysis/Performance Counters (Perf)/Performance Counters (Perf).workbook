              },
              {
                "condition": "if (TrendAggregator contains 'P10th'), result = 'asc'",
                "criteriaContext": {
                  "leftOperand": "TrendAggregator",
                  "operator": "contains",
                  "rightValType": "static",
                  "rightVal": "P10th",
                  "resultValType": "static",
                  "resultVal": "asc"
                }
              },
              {
                "condition": "else result = 'desc'",
                "criteriaContext": {
                  "operator": "Default",
                  "rightValType": "param",
                  "resultValType": "static",
                  "resultVal": "desc"
                }
              }
            ]

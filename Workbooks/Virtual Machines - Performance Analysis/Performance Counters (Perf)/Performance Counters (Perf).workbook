            "version": "KqlParameterItem/1.0",
            "name": "TrendAggregatorOrder1",
            "type": 1,
            "value": "desc",
            "isHiddenWhenLocked": true,
            "criteriaData": [
              {
                "condition": "if (TrendAggregator contains 'P5th'), result = 'asc'",
                "criteriaContext": {
                  "leftOperand": "TrendAggregator",
                  "operator": "contains",
                  "rightValType": "static",
                  "rightVal": "P5th",
                  "resultValType": "static",
                  "resultVal": "asc"

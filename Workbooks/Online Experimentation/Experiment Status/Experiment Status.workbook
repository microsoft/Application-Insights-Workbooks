            {
              "columnMatch": "DaysSinceActivity",
              "formatter": 18,
              "formatOptions": {
                "thresholdsOptions": "icons",
                "thresholdsGrid": [
                  {
                    "sourceColumn": "AssignmentEventCount",
                    "operator": "==",
                    "thresholdValue": "0",
                    "representation": "stopped",
                    "text": "Inactive",

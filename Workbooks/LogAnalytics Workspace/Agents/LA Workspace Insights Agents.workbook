            "content": {
              "version": "KqlItem/1.0",
              "query": "Heartbeat\r\n| where TimeGenerated {TimeRange:query}\r\n| summarize LastHeartbeat=max(TimeGenerated), LastVersion=max(Version) by _ResourceId, ResourceType, Computer, AgentCategory=Category\r\n| extend ActivityState = iff(datetime_diff('minute',{TimeRange:end}, LastHeartbeat) >= 60, 'Unhealthy', 'Healthy')\r\n| summarize CalculatedCount=count() by ActivityState\r\n| sort by ActivityState asc",
              "size": 3,
              "title": "Agents State",
              "noDataMessage": "No agents found",
              "timeContextFromParameter": "TimeRange",

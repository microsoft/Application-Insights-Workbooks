          {
            "type": 9,
            "content": {
              "version": "KqlParameterItem/1.0",
              "parameters": [
                {
                  "id": "7c9b89e5-24ce-4b92-9444-f865ce1d55e4",
                  "version": "KqlParameterItem/1.0",
                  "name": "AgentsState",
                  "label": "Select agents",
                  "type": 10,
                  "isRequired": true,
                  "typeSettings": {
                    "additionalResourceOptions": [],
                    "showDefault": false
                  },
                  "jsonData": "[{\"value\":\"All agents\", \"selected\":true},\r\n{\"value\":\"Healthy\"},\r\n{\"value\":\"Unhealthy\"}]",
                  "timeContext": {
                    "durationMs": 86400000
                  }
                }
              ],
              "style": "above",
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces"
            },
            "name": "AgentsState"
          },
          {
            "type": 3,
            "content": {
              "version": "KqlItem/1.0",
              "query": "let AllAgentsData = materialize(\nHeartbeat\n| where TimeGenerated {TimeRange:query}\n| summarize\n    FirstHeartbeat=min(TimeGenerated),\n    LastHeartbeat=max(TimeGenerated),\n    arg_max(TimeGenerated, OSType, OSName) \n    by\n    _ResourceId, ResourceType, Computer, AgentCategory=Category, AgentVersion=Version, SubscriptionId, lowerRG=tolower(ResourceGroup), ComputerEnvironment\n| extend ActivityState = iff(datetime_diff('minute',{TimeRange:end}, LastHeartbeat) >= 60, 'Inactive', 'Active'));\nlet RelevantAgents =\nAllAgentsData\n| summarize AgentsActivityStates=makelist(ActivityState), \n            AgentVersions=array_sort_asc(makelist(AgentVersion)),\n            ActiveAgentVersions=array_sort_asc(make_list_if(AgentVersion, ActivityState==\"Active\")),\n            LastHeartbeat=max(LastHeartbeat),\n            CurrentVersionStartTime=max(FirstHeartbeat)\n  by _ResourceId, ResourceType, Computer, AgentCategory \n| extend HealthState = iff(AgentsActivityStates has \"Active\", 'Healthy', 'Unhealthy')\n// filtering by the selected healthy state, if selected\n| where HealthState == '{AgentsState}' or '{AgentsState}'==\"All agents\";\nlet RelevantAgentsData = \nRelevantAgents \n| join kind=inner (AllAgentsData) on _ResourceId, ResourceType, Computer, AgentCategory;\nlet ActivityTrend =\nHeartbeat\n| where TimeGenerated {TimeRange:query}\n| where _ResourceId in (RelevantAgentsData)\n| make-series HeartbeatsTrend=count() on TimeGenerated from ({TimeRange:start}) to ({TimeRange:end}) step ({TimeRange:grain}) by _ResourceId, ResourceType, Computer, AgentCategory=Category, SubscriptionId, lowerRG=tolower(ResourceGroup), ComputerEnvironment;\nRelevantAgentsData\n| extend Details=case(\n    array_length(ActiveAgentVersions)==0, \n        strcat(\"Agent stopped, last heartbeat: \", format_datetime(LastHeartbeat, 'yyyy-MM-dd HH:mm:ss'), \" UTC\"),\n    array_length(AgentVersions)>1 and array_length(ActiveAgentVersions)==1, \n        strcat(\"Agent version upgraded at \", format_datetime(CurrentVersionStartTime,'yyyy-MM-dd HH:mm:ss'), \" UTC\"),\n    array_length(AgentVersions)>1 and array_length(ActiveAgentVersions)>1, \n        strcat(\"Multiple versions installed: \", strcat_array(AgentVersions, \", \")),\n    \"\")\n| summarize SubscriptionId=any(SubscriptionId), ComputerEnvironment=any(ComputerEnvironment), OSType=max(OSType), OSName=max(OSName), Details=any(Details)\n  by _ResourceId, ResourceType, Computer, AgentCategory, HealthState, AgentVersion=iff(array_length(ActiveAgentVersions)>0, strcat_array(ActiveAgentVersions, \", \"), strcat_array(AgentVersions, \",\"))\n// adding the activity trends\n| join kind=inner (ActivityTrend) on _ResourceId, ResourceType, Computer, AgentCategory, SubscriptionId, ComputerEnvironment\n// projecting fields\n| project Resource=_ResourceId, [\"Resource type\"]=ResourceType, Computer, OS=iff(strlen(OSName)>0, strcat(OSType, \" (\", OSName, \")\"), OSType), [\"Agent category\"]=AgentCategory, [\"Agent version\"]=AgentVersion, [\"Heartbeats trend\"]=HeartbeatsTrend, [\"Agent state\"]=HealthState, Details, Subscription=SubscriptionId, [\"Resource group\"]=lowerRG, Environment=ComputerEnvironment\n| sort by Resource asc, Computer asc, [\"Agent category\"] asc, [\"Agent version\"] asc\n\n",
              "size": 0,
              "showAnalytics": true,
              "timeContextFromParameter": "TimeRange",
              "showExportToExcel": true,
              "queryType": 0,
              "resourceType": "microsoft.operationalinsights/workspaces",
              "crossComponentResources": [
                "{Resource}"
              ],
              "gridSettings": {
                "formatters": [
                  {
                    "columnMatch": "Resource",
                    "formatter": 0,
                    "formatOptions": {
                      "customColumnWidthSetting": "15%"
                    }
                  },
                  {
                    "columnMatch": "Resource type",
                    "formatter": 0,
                    "formatOptions": {
                      "customColumnWidthSetting": "12%"
                    }
                  },
                  {
                    "columnMatch": "Computer",
                    "formatter": 0,
                    "formatOptions": {
                      "customColumnWidthSetting": "18%"
                    }
                  },
                  {
                    "columnMatch": "Heartbeats trend",
                    "formatter": 10,
                    "formatOptions": {
                      "palette": "green"
                    }
                  },
                  {
                    "columnMatch": "Last heartbeat",
                    "formatter": 18,
                    "formatOptions": {
                      "thresholdsOptions": "icons",
                      "thresholdsGrid": [
                        {
                          "operator": ">",
                          "thresholdValue": "3600",
                          "representation": "3",
                          "text": "{0} {1} ago"
                        },
                        {
                          "operator": ">",
                          "thresholdValue": "600",
                          "representation": "2",
                          "text": "{0} {1} ago"
                        },
                        {
                          "operator": "Default",
                          "thresholdValue": null,
                          "representation": "Blank",
                          "text": "{0} {1} ago"
                        }
                      ]
                    },
                    "numberFormat": {
                      "unit": 24,
                      "options": {
                        "style": "decimal",
                        "useGrouping": false,
                        "maximumFractionDigits": 0
                      }
                    }
                  },
                  {
                    "columnMatch": "SubscriptionId",
                    "formatter": 1
                  }
                ]
              },
              "sortBy": []
            },
            "name": "Computers missing heartbeats"
          }
        ]
      },
      "name": "Agents"
    }

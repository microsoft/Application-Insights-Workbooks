                                }
                              },
                              {
                                "columnMatch": "Median Ingestion (seconds)",
                                "formatter": 8,
                                "formatOptions": {
                                  "palette": "redDark"
                                },
                                "numberFormat": {
                                  "unit": 24,
                                  "options": {
                                    "style": "decimal",
                                    "useGrouping": false,
                                    "maximumFractionDigits": 2
                                  }
                                }
                              },
                              {
                                "columnMatch": "90th Percentile Ingestion time (seconds)",

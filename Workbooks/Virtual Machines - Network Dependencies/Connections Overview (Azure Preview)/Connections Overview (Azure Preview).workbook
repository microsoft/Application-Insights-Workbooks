            "name": "ArmPrefix",
            "type": 1,
            "isRequired": true,
            "isHiddenWhenLocked": true,
            "criteriaData": [
              {
                "condition": "if (ResourceGroups != '*'), result = '{ResourceGroups:unformatted}'",
                "criteriaContext": {
                  "leftOperand": "ResourceGroups",
                  "operator": "!=",
                  "rightValType": "static",
                  "rightVal": "*",
                  "resultValType": "static",
                  "resultVal": "{ResourceGroups:unformatted}"
                }
              },

                "condition": "else result = '[     {         \"value\": \"0\",         \"label\": \"Computer -> Process -> Remote IP -> Port\"     },     {         \"value\": \"1\",         \"label\": \"Computer -> Process -> Remote IP\"     },     {         \"value\": \"2\",         \"label\": \"Computer -> Process\", \t\t\"selected\": true     },     {         \"value\": \"3\",         \"label\": \"Computer\"     } ]'",
                "criteriaContext": {

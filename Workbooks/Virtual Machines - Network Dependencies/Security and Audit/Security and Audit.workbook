            "name": "MaliciousIpText",
            "type": 1,
            "query": "let maliciousIp = \"{CurrentMaliciousIp}\";\r\nlet maliciousIpText = iff(maliciousIp == \"\", \"\", \" - {CurrentMaliciousIp}\");\r\nprint maliciousIpText",
            "crossComponentResources": [
              "{Workspaces}"
            ],

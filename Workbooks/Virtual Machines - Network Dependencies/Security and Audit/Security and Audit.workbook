            "version": "KqlParameterItem/1.0",
            "name": "CountryName",
            "type": 1,
            "query": "let country = \"{CurrentCountry}\";\r\nlet countryText = iff(country == \"\", \"\", strcat(\" - \", country));\r\nprint countryText",

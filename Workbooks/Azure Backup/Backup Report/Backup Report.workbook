        "query": "// Time variable used\r\nlet Today = startofday(now());\r\nlet StartDate = startofday({TimeRange:start});\r\nlet EndDate = startofday({TimeRange:end}) + 1d;\r\n\r\n//Fetch Vault Table\r\nlet VaultTable = ( )\r\n{\r\n\tlet VaultTableFromAzureDiagnostics = AzureDiagnostics\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t| where OperationName == \"Vault\" and SchemaVersion_s == \"V2\"\r\n\t\t| where VaultName_s in  ({Vault}) or '*' in ({Vault})\r\n\t\t| where AzureDataCenter_s in ({Location}) or '*' in ({Location})\r\n\t\t| project ResourceId, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by ResourceId;\r\n\r\n\tlet VaultTableFromCoreAzureBackup = CoreAzureBackup\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t| where OperationName == \"Vault\" \r\n\t\t| where VaultName in ({Vault}) or '*' in ({Vault})\r\n\t\t| where AzureDataCenter in ({Location}) or '*' in ({Location})\r\n\t\t| project ResourceId, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by ResourceId;\r\n\t\t\r\n\tlet FinalVaultTable = materialize(\r\n\t\tCombinedTable | union isfuzzy = true \r\n\t\t(VaultTableFromCoreAzureBackup),\r\n\t\t(VaultTableFromAzureDiagnostics)\t\r\n\t\t| summarize arg_max(TimeGenerated, *) by ResourceId);\r\n\t\t\r\n\tFinalVaultTable\r\n\t\t| project ResourceId\r\n};\r\n\r\n\r\n//Fetch Backup Item Info Table\r\nlet BackupItemInfoTable = ( )\r\n{\r\n\tlet BackupItemAssociationFromAzureDiagnostics = AzureDiagnostics\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t| where OperationName == \"BackupItemAssociation\" and SchemaVersion_s == \"V2\" and State_s != \"Deleted\"\r\n\t\t| project BackupItemUniqueId_right = BackupItemUniqueId_s, ProtectedContainerUniqueId = ProtectedContainerUniqueId_s, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by BackupItemUniqueId_right\r\n\t\t| project-away TimeGenerated;\r\n\r\n\tlet BackupItemFromAzureDiagnostics = AzureDiagnostics\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t| where OperationName == \"BackupItem\" and SchemaVersion_s == \"V2\"\r\n\t\t| project BackupItemUniqueId = BackupItemUniqueId_s, \r\n\t\t\tBMSTypeWithBackupItemType = strcat(BackupManagementType_s, \"/\", BackupItemType_s), \t\t\t\r\n\t\t\tBackupItemName = BackupItemName_s, ResourceId, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by BackupItemUniqueId;\r\n\r\n\tlet BackupItemInfoFromAzureDiagnostics =  BackupItemFromAzureDiagnostics\r\n\t\t| join kind = inner (BackupItemAssociationFromAzureDiagnostics) on $left.BackupItemUniqueId == $right.BackupItemUniqueId_right\r\n\t\t| project-away BackupItemUniqueId_right;\r\n\r\n\t  \r\n\tlet BackupItemAssociationResourceSpecific = CoreAzureBackup\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t// Take records until previous day\r\n\t\t| where OperationName == \"BackupItemAssociation\" and State != \"Deleted\"\r\n\t\t| project BackupItemUniqueId_right = BackupItemUniqueId, ProtectedContainerUniqueId, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by BackupItemUniqueId_right\r\n\t\t| project-away TimeGenerated;\r\n\r\n\tlet BackupItemFromResourceSpecific = CoreAzureBackup\r\n\t\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t\t| where OperationName == \"BackupItem\" and State != \"Deleted\"\r\n\t\t| project BackupItemUniqueId, BMSTypeWithBackupItemType = strcat(BackupManagementType, \"/\", BackupItemType), \t\t\t\r\n\t\t\tBackupItemName, ResourceId, TimeGenerated\r\n\t\t| summarize arg_max(TimeGenerated, *) by BackupItemUniqueId;\r\n\r\n\tlet BackupItemInfoFromResourceSpecific =  BackupItemFromResourceSpecific\r\n\t\t| join kind = inner (BackupItemAssociationResourceSpecific) on $left.BackupItemUniqueId == $right.BackupItemUniqueId_right\r\n\t\t| project-away BackupItemUniqueId_right;\r\n\t\t\r\n\t\t\r\n\t\t\r\n\tlet BackupItemInfo = CombinedTable | union isfuzzy = true \r\n\t\t(BackupItemInfoFromAzureDiagnostics),\r\n\t\t(BackupItemInfoFromResourceSpecific)\t\r\n\t\t| summarize arg_max(TimeGenerated, *) by BackupItemUniqueId;\r\n\t\t\r\n\t\t\r\n\t\t\r\n\tlet BackupItemInfoInSelectedVault = VaultTable \r\n\t\t| join kind = inner (\r\n\t\t   BackupItemInfo \r\n\t\t ) on ResourceId == ResourceId;\r\n\t\t \r\n\t\t\r\n\t\t\r\n\tBackupItemInfoInSelectedVault\r\n\t\t| project ProtectedContainerUniqueId, BackupItemName, BackupItemUniqueId, BMSTypeWithBackupItemType;\r\n};\r\n\r\n\r\n//Fetch Protected Container Info Table\r\nlet ProtectedContainerInfoTable = ()\r\n{\r\n\tlet ProtectedContainerFromAzureDiagnostics = AzureDiagnostics\r\n\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t| where OperationName == \"ProtectedContainer\" and SchemaVersion_s == \"V2\"\r\n\t| project BMSTypeWithBackupItemType = strcat(BackupManagementType_s, \"/\"), ProtectedContainerUniqueId = ProtectedContainerUniqueId_s, ProtectedContainerName = ProtectedContainerName_s, ResourceId, TimeGenerated\t\t\r\n\t| summarize arg_max(TimeGenerated, *) by ProtectedContainerUniqueId;\r\n\r\n\tlet ProtectedContainerInfoFromAzureDiagnostics =  ProtectedContainerFromAzureDiagnostics;\r\n\r\n\r\n\tlet ProtectedContainerFromResourceSpecific = CoreAzureBackup\r\n\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t| where OperationName == \"ProtectedContainer\" and State != \"Deleted\"\r\n\t| project BMSTypeWithBackupItemType = strcat(BackupManagementType, \"/\"), ProtectedContainerUniqueId, ProtectedContainerName, ResourceId, TimeGenerated\t\t\r\n\t| summarize arg_max(TimeGenerated, *) by ProtectedContainerUniqueId;\r\n\r\n\tlet ProtectedContainerInfoFromResourceSpecific =  ProtectedContainerFromResourceSpecific;\r\n\t\r\n\t\r\n\tlet ProtectedContainerInfo = CombinedTable | union isfuzzy = true \r\n\t(ProtectedContainerInfoFromAzureDiagnostics),\r\n\t(ProtectedContainerInfoFromResourceSpecific)\t\r\n\t| summarize arg_max(TimeGenerated, *) by ProtectedContainerUniqueId;\r\n\t\t\r\n\t\r\n\t\r\n    let ProtectedContainerInfoInSelectedVault = VaultTable \r\n\t| join kind = inner (\r\n       ProtectedContainerInfo \r\n     ) on ResourceId == ResourceId;\r\n\t \r\n\t \r\n\t \r\n\tProtectedContainerInfoInSelectedVault\r\n \t| project ProtectedContainerUniqueId, ProtectedContainerName, BMSTypeWithBackupItemType;\r\n};\r\n\r\n\r\n// Fetch BackupManagementServer Info Table\r\nlet BackupManagementServerInfoTable = ()\r\n{\r\n\tlet BackupManagementServerFromAzureDiagnostics = AzureDiagnostics\r\n\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t| where OperationName == \"BackupManagementServer\" and SchemaVersion_s == \"V2\"\r\n    | project BMSTypeWithBackupItemType = strcat(BackupManagementType_s, \"/\"), BackupManagementServerUniqueId = strcat(column_ifexists(\"BackupManagementServerUniqueId_s\", \"\"), column_ifexists(\"BackupManagmentServerUniqueId_s\", \"\")),BackupManagementServerName = strcat(column_ifexists(\"BackupManagementServerName_s\", \"\"), column_ifexists(\"BackupManagmentServerName_s\", \"\")), ResourceId, TimeGenerated\t\r\n\t| summarize arg_max(TimeGenerated, *) by BackupManagementServerUniqueId;\r\n\r\n\tlet BackupManagementServerInfoFromAzureDiagnostics =  BackupManagementServerFromAzureDiagnostics;\r\n\t\r\n\t\r\n\r\n\tlet BackupManagementServerFromResourceSpecific = CoreAzureBackup\r\n\t| where TimeGenerated >= StartDate and TimeGenerated <= EndDate and TimeGenerated < Today\r\n\t| where OperationName == \"BackupManagementServer\" and State != \"Deleted\"\r\n\t| project BMSTypeWithBackupItemType = strcat(BackupManagementType, \"/\"), BackupManagementServerUniqueId, BackupManagementServerName, ResourceId, TimeGenerated\r\n\t| summarize arg_max(TimeGenerated, *) by BackupManagementServerUniqueId;\r\n\r\n\tlet BackupManagementServerInfoFromResourceSpecific =  BackupManagementServerFromResourceSpecific;\r\n\r\n\t\r\n\t\t\r\n\tlet BackupManagementServerInfo = CombinedTable | union isfuzzy = true \r\n\t(BackupManagementServerInfoFromAzureDiagnostics),\r\n\t(BackupManagementServerInfoFromResourceSpecific)\t\r\n\t| summarize arg_max(TimeGenerated, *) by BackupManagementServerUniqueId;\r\n\t\r\n\t\r\n\t\r\n\t\r\n    let BackupManagementServerInfoInSelectedVault = VaultTable \r\n\t| join kind = inner (\r\n       BackupManagementServerInfo \r\n     ) on ResourceId == ResourceId;\r\n\t \r\n\t \r\n\t \r\n\t \r\n\tBackupManagementServerInfoInSelectedVault\r\n\t| project BackupManagementServerUniqueId, BackupManagementServerName, BMSTypeWithBackupItemType \r\n};\r\n\r\n// Fetch BackupItem With ProtectedContainer Name\r\nlet BackupItemWithProtectedContainer = ( )\r\n{\r\n\tlet BackupItemWithProtectedContainerName = ProtectedContainerInfoTable\r\n\t| join kind = rightouter (BackupItemInfoTable) on ProtectedContainerUniqueId == ProtectedContainerUniqueId\r\n\t\t| project ContainerNameWithBackupItemName = replace(\" \", \"\", strcat(ProtectedContainerName, @\"/\", BackupItemName)), BackupItemUniqueId, BMSTypeWithBackupItemType = BMSTypeWithBackupItemType1;\r\n\r\n    BackupItemWithProtectedContainerName\t\r\n};\r\n\r\n\r\n// Fetch Alert Info Table\r\nlet AlertInfoTable = ( )\r\n{\r\n\tlet AlertFromAzureDiagnostics = AzureDiagnostics\t\r\n\t| where TimeGenerated >= StartDate and TimeGenerated < Today\r\n\t| where OperationName == \"Alert\" and SchemaVersion_s == \"V2\" \r\n\t| project AlertUniqueId = strcat(column_ifexists(\"AlertUniqueId_g\", \"\"), column_ifexists(\"AlertUniqueId_s\", \"\")), BackupItemUniqueId = column_ifexists(\"BackupItemUniqueId_s\", \"\"), AlertStatus  = column_ifexists(\"AlertStatus_s\", \"\"), ProtectedContainerUniqueId = column_ifexists(\"ProtectedContainerUniqueId_s\", \"\"), BackupManagementServerUniqueId = column_ifexists(\"BackupManagementServerUniqueId_s\", \"\"), AlertType = column_ifexists(\"AlertType_s\", \"\"), AlertSeverity = column_ifexists(\"AlertSeverity_s\", \"\"), AlertTimeToResolveInMinutes = toreal(column_ifexists(\"AlertTimeToResolveInMinutes_s\", \"\")), AlertOccurrenceDateTime = todatetime(column_ifexists(\"AlertOccurrenceDateTime_s\", \"\")), ResourceId, TimeGenerated\r\n\t| summarize arg_max(TimeGenerated, *) by AlertUniqueId;\r\n\r\n\t\r\n\tlet AlertFromResourceSpecific = AddonAzureBackupAlerts\r\n\t| where TimeGenerated >= StartDate and TimeGenerated < Today\r\n\t| where OperationName == \"Alert\" \r\n\t| project AlertUniqueId, BackupItemUniqueId, AlertStatus, ProtectedContainerUniqueId, BackupManagementServerUniqueId, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, AlertOccurrenceDateTime, ResourceId, TimeGenerated\r\n\t| summarize arg_max(TimeGenerated, *) by AlertUniqueId;\r\n\r\n\t\r\n\tlet AlertInfo = CombinedTable | union isfuzzy = true \r\n\t(AlertFromAzureDiagnostics),\r\n\t(AlertFromResourceSpecific)\t\r\n\t| summarize arg_max(TimeGenerated, *) by AlertUniqueId\r\n\t| where AlertOccurrenceDateTime >= StartDate and AlertOccurrenceDateTime <= EndDate;\r\n\t\r\n\t\r\n\tlet AlertInfoInSelectedVault = VaultTable \r\n\t| join kind = inner (\r\n       AlertInfo \r\n     ) on ResourceId == ResourceId;\r\n\t \r\n\t \r\n\tAlertInfoInSelectedVault\t\r\n};\r\n\r\n\r\n//**********************************************************************************************End of High-level Functions**********************************************************************************************\r\n\r\n\r\n//**********************************************************************************************Business Logic************************************************************************************************************\r\nlet AlertWithAlertSpecificFilters = ( )\r\n{\r\n\t AlertInfoTable\r\n\t | summarize arg_max(TimeGenerated, *)  by AlertUniqueId\r\n\t | where AlertType in ({AlertType})\r\n     | project AlertUniqueId, AlertStatus, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, BackupManagementServerUniqueId, ProtectedContainerUniqueId, BackupItemUniqueId, AlertOccurrenceDateTime, TimeGenerated;\r\n};\r\n\r\nlet AlertUnderBackupItem = ()\r\n{\r\n\tAlertWithAlertSpecificFilters\r\n\t| where BackupItemUniqueId != \"\"\r\n\t| join kind = inner ( BackupItemWithProtectedContainer) on BackupItemUniqueId == BackupItemUniqueId\r\n    | project AlertUniqueId, AlertStatus, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, BackupManagementServerUniqueId, ProtectedContainerUniqueId, BackupItemUniqueId, ContainerNameWithBackupItemName, BMSTypeWithBackupItemType, AlertOccurrenceDateTime, TimeGenerated;\r\n};\r\n\r\nlet AlertUnderProtectedContainer = ()\r\n{\r\n\tAlertWithAlertSpecificFilters\r\n\t| where BackupItemUniqueId == \"\" and ProtectedContainerUniqueId != \"\"\r\n\t| join kind = innner ( ProtectedContainerInfoTable) on ProtectedContainerUniqueId == ProtectedContainerUniqueId\r\n\t| extend ContainerNameWithBackupItemName = ProtectedContainerName, BMSTypeWithBackupItemType = BMSTypeWithBackupItemType\r\n    | project AlertUniqueId, AlertStatus, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, BackupManagementServerUniqueId, ProtectedContainerUniqueId, BackupItemUniqueId, ContainerNameWithBackupItemName, BMSTypeWithBackupItemType, AlertOccurrenceDateTime, TimeGenerated;\r\n};\r\n\r\nlet AlertUnderBackupManagementServer = ()\r\n{\r\n\tAlertWithAlertSpecificFilters\r\n\t| where BackupItemUniqueId == \"\" and ProtectedContainerUniqueId == \"\"\r\n\t| join kind = innner ( BackupManagementServerInfoTable) on BackupManagementServerUniqueId == BackupManagementServerUniqueId\r\n\t| extend ContainerNameWithBackupItemName = BackupManagementServerName, BMSTypeWithBackupItemType = BMSTypeWithBackupItemType\r\n    | project AlertUniqueId, AlertStatus, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, BackupManagementServerUniqueId, ProtectedContainerUniqueId, BackupItemUniqueId, ContainerNameWithBackupItemName, BMSTypeWithBackupItemType, AlertOccurrenceDateTime, TimeGenerated;\r\n};\r\n\r\n\r\nlet AlertTableWithAppliedFilter = materialize\r\n(CombinedTable | union isfuzzy = true \r\n(AlertUnderBackupItem()),\r\n(AlertUnderProtectedContainer()),\r\n(AlertUnderBackupManagementServer())\r\n| where BMSTypeWithBackupItemType in ({BMSTypeWithBackupItemTypeParam}) or '*' in ({BMSTypeWithBackupItemTypeParam})\r\n| distinct *\r\n| project AlertUniqueId, AlertStatus, AlertType, AlertSeverity, AlertTimeToResolveInMinutes, ContainerNameWithBackupItemName, BMSTypeWithBackupItemType, AlertOccurrenceDateTime, TimeGenerated\r\n);\r\nlet FinalAlert = ()\r\n{\r\n  AlertTableWithAppliedFilter \r\n  | where AlertUniqueId != \"\"\r\n  | summarize count() by AlertSeverity;\r\n};\r\nFinalAlert",

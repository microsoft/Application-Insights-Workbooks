          },
          {
            "id": "df3cc03c-53f7-4523-a29b-e50bf1b6d922",
            "version": "KqlParameterItem/1.0",
            "name": "JobMetricOrder2",
            "label": "Order",
            "type": 2,
            "description": "Use to specify the sort order: Ascending/Descending",
            "isRequired": true,
            "typeSettings": {

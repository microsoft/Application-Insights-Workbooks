          },
          {
            "id": "595e5629-4f64-4038-bff5-5f3371259caa",
            "version": "KqlParameterItem/1.0",
            "name": "BillingMetricOrder2",
            "label": "Order",
            "type": 2,
            "description": "Use to specify sort order: Ascending/Descending",
            "isRequired": true,
            "typeSettings": {

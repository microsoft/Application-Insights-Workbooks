            "quote": "'",
            "delimiter": ",",
            "query": "resources\r\n|Â whereÂ typeÂ =~Â '{ResourceType}'\r\n| where name =~ \"{Resource:Name}\"\r\n| extend workspaceResourceIdÂ =Â tostring(parse_json(tolower(properties)).addonprofiles.omsagent.config.loganalyticsworkspaceresourceid)\r\n| project workspaceResourceIdÂ ",
            "crossComponentResources": [

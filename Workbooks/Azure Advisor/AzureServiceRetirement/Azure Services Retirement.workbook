nc6_promo','standard_nc6')  or tolower(tostring(sku.name)) in ('standard_nc24r_promo','standard_nc24_promo','standard_nc24r','standard_nc12_promo','standard_nc12','standard_nc24','standard_nc6_promo','standard_nc6')),18\r\n,type == \"microsoft.datalakestore/accounts\" and properties.migrationInfo.state == \"RequestConsent\",19\r\n,type == 'microsoft.classicnetwork/reservedips',20\r\n,type == 'microsoft.classicnetwork/networksecuritygroups',21\r\n,-9999)\r\n| where ServiceID >0\r\n| project ServiceID , subscriptionId,type, resourceGroup, location, id \r\n|union \r\n// Query for Classic Redis caches retired\r\n(AdvisorResources\r\n| where type =='microsoft.advisor/recommendations'\r\n| where properties.shortDescription contains 'Cloud service caches are being retired in August 2024'\r\n| project id=tolower(tostring(properties.resourceMetadata.resourceId))\r\n| join \r\n(resources\r\n| where type contains 'microsoft.cache/redis'\r\n| project id=tolower(id), subscriptionId, resourceGroup, location,type\r\n) on id\r\n| project ServiceID=22 , subscriptionId,type, resourceGroup, location, id )\r\n| where resourceGroup in ({ResourceGroup}) and location in ({Region})\r\n| where array_length(dynamic([{SelectedServiceId}])) == 0 or ServiceID in (dynamic([{SelectedServiceId}])) \r\n| summarize ResourceCount = count() by Region=location",
